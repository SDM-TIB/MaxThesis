@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix fr: <http://FrenchRoyalty.org/>.
@prefix dbr: <http://dbpedia.org/resource/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.

fr:Person  a  rdfs:Class .
fr:Spouse  a  rdfs:Class .

fr:Spouse rdfs:subClassOf fr:Person.
 
fr:father a rdfs:Property , owl:ObjectProperty;
    rdfs:domain fr:Person ;
    rdfs:range  fr:Person .

fr:hasSpouse a rdfs:Property , owl:ObjectProperty;
    rdfs:domain fr:Person ;
    rdfs:range  fr:Person .

foaf:name a rdfs:Property , owl:DatatypeProperty;
    rdfs:domain fr:Person ;
    rdfs:range  xsd:anyType .

fr:mother a rdfs:Property , owl:ObjectProperty;
    rdfs:domain fr:Person ;
    rdfs:range  fr:Person .

fr:parent a rdfs:Property , owl:ObjectProperty;
    rdfs:domain fr:Person ;
    rdfs:range  fr:Person .

fr:successor a rdfs:Property , owl:ObjectProperty;
    rdfs:domain fr:Person ;
    rdfs:range  fr:Person .

fr:predecessor a rdfs:Property , owl:ObjectProperty;
    rdfs:domain fr:Person ;
    rdfs:range  fr:Person .

fr:gender a rdfs:Property , owl:DatatypeProperty;
    rdfs:domain fr:Person ;
    rdfs:range  xsd:anyType .

fr:child a rdfs:Property , owl:ObjectProperty;
    rdfs:domain fr:Person ;
    rdfs:range  fr:Person .

fr:spouse a rdfs:Property , owl:ObjectProperty;
    rdfs:domain fr:Person ;
    rdfs:range  fr:Person ;
    rdfs:subPropertyOf fr:marriedTo .

fr:marriedTo a rdfs:Property , owl:ObjectProperty;
    rdfs:domain fr:Person ;
    rdfs:range  fr:Person .
    
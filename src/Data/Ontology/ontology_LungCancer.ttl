@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix lc: <http://synthetic-LC.org/lungCancer/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.

lc:Patient  a  rdfs:Class .
lc:Answer   a  rdfs:Class .
lc:Drug     a  rdfs:Class .
lc:Comorbidity  a  rdfs:Class .
lc:Biomarker    a  rdfs:Class .
lc:Stage        a  rdfs:Class .
lc:Sex          a  rdfs:Class .
lc:AgeCategory  a rdfs:Class .

lc:stage a rdfs:Property , owl:ObjectProperty;
    rdfs:domain lc:Patient ;
    rdfs:range  lc:Stage .

lc:smokingHabit a rdfs:Property , owl:ObjectProperty;
    rdfs:domain lc:Patient ;
    rdfs:range  lc:Answer .

lc:biomarker a rdfs:Property , owl:ObjectProperty;
    rdfs:domain lc:Patient ;
    rdfs:range  lc:Biomarker .

lc:sex a rdfs:Property , owl:ObjectProperty;
    rdfs:domain lc:Patient ;
    rdfs:range  lc:Sex .

lc:ageCategory a rdfs:Property , owl:ObjectProperty;
    rdfs:domain lc:Patient ;
    rdfs:range  lc:AgeCategory .

lc:comorbidity a rdfs:Property , owl:ObjectProperty;
    rdfs:domain lc:Patient ;
    rdfs:range  lc:Comorbidity .

lc:hasRelapse a rdfs:Property , owl:ObjectProperty;
    rdfs:domain lc:Patient ;
    rdfs:range  lc:Answer .

lc:drug a rdfs:Property , owl:ObjectProperty;
    rdfs:domain lc:Patient ;
    rdfs:range  lc:Drug .


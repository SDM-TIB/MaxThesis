@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix lc: <http://LungCancer.org/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.

lc:Patient  a  rdfs:Class .
lc:LCPatient  a  rdfs:Class .

lc:LCPatient rdfs:subClassOf lc:Patient.
 
lc:hasStage a rdfs:Property , owl:ObjectProperty;
    rdfs:domain lc:Patient ;
    rdfs:range  lc:Patient .

lc:hasSmokingHabit a rdfs:Property , owl:ObjectProperty;
    rdfs:domain lc:Patient ;
    rdfs:range  lc:Patient .

lc:treatmentType a rdfs:Property , owl:ObjectProperty;
    rdfs:domain lc:Patient ;
    rdfs:range  lc:Patient .

lc:hasBiomarker a rdfs:Property , owl:ObjectProperty;
    rdfs:domain lc:Patient ;
    rdfs:range  lc:Patient .

lc:hasRelapse_Progression a rdfs:Property , owl:ObjectProperty;
    rdfs:domain lc:Patient ;
    rdfs:range  lc:Patient .

lc:patientDrug a rdfs:Property , owl:ObjectProperty;
    rdfs:domain lc:Patient ;
    rdfs:range  lc:Patient .

lc:hasGender a rdfs:Property , owl:ObjectProperty;
    rdfs:domain lc:Patient ;
    rdfs:range  lc:Patient .

lc:hasAgeCategory a rdfs:Property , owl:ObjectProperty;
    rdfs:domain lc:Patient ;
    rdfs:range  lc:Patient .